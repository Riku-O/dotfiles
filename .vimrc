"vunlde.vimで管理してるpluginを読み込む
source ~/dotfiles/.vimrc.bundle

"基本設定
source ~/dotfiles/.vimrc.basic

"インデント設定
source ~/dotfiles/.vimrc.indent
 
"表示関連
source ~/dotfiles/.vimrc.apperance

